package object.Repository;

import org.openqa.selenium.By;

public class EntitiesRepo
{
  public static By entities=By.xpath(".//*[contains(@class,'hidden-tablet ng-scope')]");
  public static By branch=By.xpath(".//*[contains(@class,'global.menu.entities.branch')]");
  public static By create_Branch=By.className("btn btn-primary");
